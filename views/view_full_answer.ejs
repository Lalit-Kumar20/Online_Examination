<% for(let i = 0;i<ques.length;++i){ %>
    <h1>Q. <%= i+1 %> </h1>
<h2><%= ques[i].q %> </h2>
<% if(ques[i].type==="mcq") {%>
   <h4><%= ques[i].a %> </h4>
   <h4><%= ques[i].b %> </h4>
   <h4><%= ques[i].c %> </h4>
   <h4><%= ques[i].d %> </h4>
    <% } %>  
    <% if(ques[i].im==="yes"){ %>
        <% for(let j = 0;j<ques[i].img.length;++j){ %>
            <img src="/<%=ques[i].img[j]%>" alt="" width="100" height="100">
            <% } %>  
        <% } %>
       <% if(Object.keys(ans[i]).length!==0){ %>
        
        <h3>Answer : -</h3>
        <h4><%= ans[i].answer %> </h4>
        <% for(let j = 0;j<ans[i].img.length;++j){ %>
            <img src="/<%= ans[i].img[j]%>" alt="" width="100" height="100">
            <% } %>  
        <% } %>
        <hr> 
    <% } %>   